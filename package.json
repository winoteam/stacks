{
  "name": "@mobily/stacks",
  "description": "âš¡ Build React Native layouts quickly with ease and fun.",
  "version": "1.2.1",
  "license": "MIT",
  "author": "Marcin Dziewulski <hello@mobily.pl>",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/types/src/Stacks.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/types/src/Stacks.d.ts"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "src/!(*.tsx|*.bs.js)",
    "dist",
    "*.md",
    "index.js.flow",
    "bsconfig.json",
    "package.json",
    "babel"
  ],
  "scripts": {
    "build": "bash ./scripts/build.sh",
    "esbuild": "node esbuild.config.js",
    "clean": "rm -rf ./dist && rm -f ./tsconfig.tsbuildinfo",
    "test": "yarn jest --coverage",
    "re:clean": "yarn bsb -clean-world",
    "re:build": "yarn bsb -make-world",
    "re:dev": "yarn bsb -make-world -w",
    "generate:flow": "node ./scripts/generate-flow-files.js",
    "generate:contributors": "yarn all-contributors generate"
  },
  "engines": {
    "node": ">= 10.*"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react-native",
    "react",
    "typescript",
    "rescript",
    "flow",
    "stacks",
    "layout",
    "ui"
  ],
  "homepage": "https://mobily.github.io/stacks",
  "bugs": "https://github.com/mobily/stacks/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/mobily/stacks.git"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-native": ">=0.59.0"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "0.6.0",
    "@types/jest": "^26.0.23",
    "@types/react": "16.9.55",
    "@types/react-native": "0.63.32",
    "all-contributors-cli": "^6.20.0",
    "bs-platform": "^9.0.2",
    "coveralls": "^3.1.0",
    "esbuild": "^0.12.1",
    "flow-bin": "^0.151.0",
    "flowgen": "^1.14.1",
    "gentype": "^3.50.0",
    "globby": "^11.0.3",
    "jest": "^26.6.3",
    "jscodeshift": "^0.12.0",
    "minimatch": "^3.0.4",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "react-native": "^0.64.1",
    "react-native-testing-library": "^6.0.0",
    "react-test-renderer": "^17.0.2",
    "reason-react": "0.9.1",
    "reason-react-native": "^0.64.0",
    "reason-test-framework": "^0.3.2",
    "typescript": "^4.2.4",
    "wonka": "^4.0.15"
  },
  "cacheDirectories": [
    "node_modules"
  ]
}
